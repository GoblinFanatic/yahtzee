public class Yahtzee {
	public static void main(String[] args) {
		println("Welcome to Yahtzee!"
		public int score = 0;
		dice = new int[5];
		locked = new boolean[5];
	
	
	}
	public static void turn(){
		for (int i = 1; i <=5; i++) {
			if (locked[i] == false) {
				dice[i] = (int)(Math.Random()*6) +1;
			}
    		 	println("Dice" + i + ": " + dice[i]);
		}
		println("type the numbers of any dice you would like to lock");
	}
}
